doctype html
html
    head
        meta(charset="utf-8")
        style.
            li {
                padding: 10px;
                list-style: none;
            }
    body
        ul
            li
                input(type="button" onclick="open1()" value="window.open('/jwb/demo2.html');")
            li
                input(type="button" onclick="open2()" value="window.open('/jwb/demo2.html', '_self');")
                // self는 자기 창에서 새로이 리디렉션된다.
            li
                input(type="button" onclick="open3()" value="window.open('/jwb/demo2.html', '_blank');")
            li
                input(type="button" onclick="open4()" value="window.open('/jwb/demo2.html', 'ot');")
            li
                input(type="button" onclick="open5()" value="window.open('/jwb/demo2.html', '_blank', 'width=200, height=200, resizable=yes');")
        script.
            function open1() {
                window.open('/jwb/demo2.html');]
            }
            function open2() {
                window.open('/jwb/demo2.html', '_self');
            }
            function open3() {
                window.open('/jwb/demo2.html', '_blank');
            }
            function open4(){
                window.open('/jwb/demo2.html', 'ot');
            }
            function open5() {
                window.open('/jwb/demo2.html', '_blank', 'width=200, height=200, resizable=yes');
            }